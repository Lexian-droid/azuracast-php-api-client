{
    "name": "lexiandev/azuracast-php-api-client",
    "version": "2.0.0",
    "description": "AzuraCast is a standalone, turnkey web radio management tool. Radio stations hosted by AzuraCast expose a public API for viewing now playing data, making requests and more.",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/Lexian-droid/azuracast-php-api-client",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "LexianDEV",
            "homepage": "https://lexian.dev"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "AzuraCast\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "AzuraCast\\Test\\" : "test/" }
    }
}
